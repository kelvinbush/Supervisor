<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Supervisor" parent="Theme.MaterialComponents.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/surface</item>
        <item name="colorPrimaryVariant">@color/status</item>
        <item name="android:colorForeground">@color/white</item>
<!--        <item name="colorOnSurface">#</item>-->
        <item name="colorOnPrimary">@color/back</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
<!--        <item name="actionBarStyle">@style/MyActionBar</item>-->
    </style>

